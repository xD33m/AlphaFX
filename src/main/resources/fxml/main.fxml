<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextArea?>
<?import com.jfoenix.controls.JFXToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            stylesheets="@../css/dark.theme.css" xmlns="http://javafx.com/javafx/10.0.1"
            fx:controller="com.sample.mainPanel.mainController">
    <children>
        <JFXToggleButton fx:id="scannerOn" layoutX="300.0" layoutY="289.0" onAction="#handleScannerOn"
                         styleClass="text-field" text="Scanner On"/>
        <JFXTextArea fx:id="chatInfoArea" editable="false" layoutX="14.0" layoutY="249.0" prefHeight="138.0"
                     prefWidth="273.0" styleClass="text-field" wrapText="true"/>
        <JFXButton fx:id="chatConfiguration" buttonType="RAISED" layoutX="14.0" layoutY="14.0"
                   onAction="#handleChatConfiguration" styleClass="jfx-button" text="Chat Configuration"/>
        <JFXButton fx:id="ocrStart" layoutX="14.0" layoutY="39.0" text="OCR Start" onAction="#handleStartOcr"/>
    </children>
</AnchorPane>
